{
    "id" : "org.kuchelmeister.ToolboxTuner",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "46",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable",
        "org.freedesktop.Sdk.Extension.llvm15"
    ],
    "command" : "toolbox-tuner",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--talk-name=org.freedesktop.Flatpak"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin"
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "toolbox-tuner",
            "builddir" : true,
            "buildsystem": "meson",
            "build-options": {
                "env": {
                    "CARGO_HOME": "/run/build/toolbox-tuner/cargo"
                }
            },
            "config-opts": [
                "-Dprofile=default"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://github.com/13hannes11/toolbox-tuner/archive/refs/tags/dev.tar.gz",
                    "sha256" : "92b00be0b77ed8090979f4c6b4c91d2257d5c2ec56d727299b9b33a722df991c"
                },
                {
                    "type": "archive",
                    "url": "https://github.com/13hannes11/toolbox-tuner/releases/download/dev/vendor.tar.gz",
                    "sha256": "d8015fc365496a896f1d1a1dc837bf44be5cd5a98f9466797fec4a11150719b2"
                }
            ]
        }
    ]
}
