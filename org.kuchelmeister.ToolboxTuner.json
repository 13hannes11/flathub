{
    "id" : "org.kuchelmeister.ToolboxTuner",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "46",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable",
        "org.freedesktop.Sdk.Extension.llvm18"
    ],
    "command" : "toolbox-tuner",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--talk-name=org.freedesktop.Flatpak"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin"
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "toolbox-tuner",
            "builddir" : true,
            "buildsystem": "meson",
            "build-options": {
                "env": {
                    "CARGO_HOME": "/run/build/toolbox-tuner/cargo"
                }
            },
            "config-opts": [
                "-Dprofile=default"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://github.com/13hannes11/toolbox-tuner/archive/refs/tags/v0.1.0.tar.gz",
                    "sha256" : "dd6bd041a357297f7e6944484e4db5165ab204ebda727edcf7a93f6a598e16e7"
                },
                {
                    "type": "archive",
                    "url": "https://github.com/13hannes11/toolbox-tuner/releases/download/v0.1.0/vendor.tar.gz",
                    "sha256": "633af8033383fced07de6aa7d4487cc97f8ec54786b9474a51ec8071d8a6c9e0"
                }
            ]
        }
    ]
}
