{
    "id" : "org.kuchelmeister.ToolboxTuner.Devel",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "46",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable",
        "org.freedesktop.Sdk.Extension.llvm15"
    ],
    "command" : "toolbox-tuner",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--talk-name=org.freedesktop.Flatpak"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "build-args" : []
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "toolbox-tuner",
            "builddir" : true,
            "buildsystem": "meson",
            "build-options": {
                "env": {
                    "CARGO_HOME": "/run/build/toolbox-tuner/cargo"
                }
            },
            "config-opts": [
                "-Dprofile=flathub"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://github.com/13hannes11/toolbox-tuner/archive/refs/tags/dev.tar.gz",
                    "sha256" : "6ad0c0b07ebdbe5733baf4e6ec2a3c60e1976534a9f7286e998f300b32cef2e8"
                },
                {
                    "type": "archive",
                    "url": "https://github.com/13hannes11/toolbox-tuner/releases/download/dev/vendor.tar.gz",
                    "sha256": "05fa53750d7c0d47e14aa9ddc4a350034d967420a2d0ff8900d0e70f46a2dde5",
                    "strip-components": 2
                }
            ]
        }
    ]
}
